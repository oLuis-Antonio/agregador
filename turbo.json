{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
        "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "dependsOn": [],
      "persistent": true,
      "cache": false
    },
    "lint": {}
  }
}
